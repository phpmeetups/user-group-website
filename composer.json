{
    "name": "phpmeetups/user-group-website",
    "type": "project",
    "description": "Starter project for user group websites.",
    "keywords": [
        "php",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bill Condo",
            "email": "billcondo@gmail.com",
            "homepage": "https://billcondo.com"
        },
        {
            "name": "Vince Mitchell",
            "email": "vincent.mitchell@gmail.com",
            "homepage": "https://vincemitchell.me"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^2.2",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpunit/phpunit": "^9.3.3",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": "parallel-lint app config routes",
        "phpcs": "vendor/bin/phpcs",
        "phpunit": "phpunit --verbose --colors=always",
        "test": [
            "@lint",
            "@phpunit",
            "@phpcs"
        ]
    },
    "support": {
        "issues": "https://github.com/phpmeetups/user-group-website/issues"
    }
}
